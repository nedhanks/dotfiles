(
 ("default" . ((user-emacs-directory . "~/.emacs.d")
	       (env . (("EMACSDIR" . "~/.emacs.d")))))

 ("alex" .    ((user-emacs-directory . "~/.emacs.d")
	       (custom-file . "~/.emacs.d/custom.el")
               (env . (("EMACSDIR" . "~/.emacs.d")))))

 ("doom" .    ((user-emacs-directory . "~/.doom-emacs.d")
	       (env . (("EMACSDIR" . "~/.doom-emacs.d")
		       ("DOOMDIR" . "~/.doom"))))))
